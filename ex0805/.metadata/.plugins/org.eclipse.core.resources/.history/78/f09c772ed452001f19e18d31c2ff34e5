package ch02.sec01;

public class ByteExam {

	public static void main(String[] args) {
		
//		byte 범위: -128 ~ 127
		byte var1 = -128;
		byte var2 = -30;
		byte var3 = 0;
		byte var4 = 30;
		byte var5 = 127;
//		큰 변수에 담는다
		int var6 = 128;
		
		System.out.println(var1);
		System.out.println(var2);
		System.out.println(var3);
		System.out.println(var4);
		System.out.println(var5);
	}

}
